    "siblings_only": true
  },
  "MD033": {
    "allowed_elements": ["br", "sub", "sup", "kbd", "img", "details", "summary", "table", "tr", "td", "th", "thead", "tbody", "invoke", "parameter", "antml:invoke", "antml:parameter"]
  },
