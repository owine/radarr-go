{
  "dashboard": {
    "id": null,
    "title": "Radarr Go Development Dashboard",
    "tags": ["radarr", "development", "golang"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "API Request Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"radarr-go\"}[5m])",
            "format": "time_series",
            "legendFormat": "{{method}} {{handler}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"radarr-go\"}[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=\"radarr-go\"}[5m]))",
            "legendFormat": "50th percentile"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "go_memstats_alloc_bytes{job=\"radarr-go\"} / 1024 / 1024",
            "legendFormat": "Allocated Memory (MB)"
          },
          {
            "expr": "go_memstats_heap_inuse_bytes{job=\"radarr-go\"} / 1024 / 1024",
            "legendFormat": "Heap In Use (MB)"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Goroutines",
        "type": "stat",
        "targets": [
          {
            "expr": "go_goroutines{job=\"radarr-go\"}",
            "legendFormat": "Active Goroutines"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "Database Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "go_sql_open_connections{job=\"radarr-go\"}",
            "legendFormat": "Open Connections"
          },
          {
            "expr": "go_sql_in_use_connections{job=\"radarr-go\"}",
            "legendFormat": "In Use Connections"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "HTTP Status Codes",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"radarr-go\"}[5m])",
            "legendFormat": "{{status}} - {{method}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      }
    ],
    "time": {"from": "now-1h", "to": "now"},
    "refresh": "5s",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}
